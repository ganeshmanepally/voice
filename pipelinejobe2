pipeline{
    agent any
    stages{
        stage('git checkout'){
            steps{
                git url: 'https://github.com/ganeshmanepally/voice.git', branch: 'main'
            }
        }
        stage(build){
            steps {
                sh 'mvn clean install'
            }
        }
        stage("checking path"){
            steps {
                sh 'pwd'
            }
        }
    }
    post {
        always {
            cleanWs()
        }
    }
}