pipeline{
    agent any
    stages{
        stage(git checkout){
            steps{
                git url: 'https://github.com/ganeshmanepally/voice.git', branch: 'master'
            }
        }
        stage(build){
            steps {
                sh mkdir test
                sh echo "hello" >> sample.txt
            }
        }
        stage(checking path){
            steps {
                sh pwd
            }
        }
    }
}